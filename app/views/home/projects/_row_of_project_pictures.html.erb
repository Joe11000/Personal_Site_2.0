<div class="row" id="project-pictures-row">
  <% project.pictures.each do |picture| %>
    <div class="col-xs-<%= (12 / project.pictures.size).floor %> col-sm-<%= (12 / project.pictures.size).floor %> text-center project-pictures">
      <%= image_tag picture, height: "100%"%>
    </div>
  <% end %>
  <div class="clearfix"></div>
</div>
